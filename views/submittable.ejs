<%- include('header') %>


<style>
    table, th, td {
  border: 1px solid black;
  margin-top:20%;
}
 
 
</style>


<div class="row justify-content-md-center">
        <h1 style="text-align: center; margin-top:5%">Submitted PROFILE Data</h1>
    </div>

<div class="row justify-content-md-center">
    
    

    <div class="col-md-6">
<table class="table" >

    <tr>

        <th scope="col">Firstname</th>
        <td><%= fname %></td>
    </tr>

    <tr>
        <th scope="col">Lastname</th>
        <td><%= lname %></td> 
    </tr>

    
    <tr>
        <th scope="col">Email</th>
        <td><%= email %></td>
    </tr>

    
    <tr>
        <th scope="col">Number</th>
        
        <td><%= mobile %></td>
    </tr>

    
    <tr>
        <th scope="col">Birthday</th>
        <td><%= birthdate %></td>
    </tr>

    
    <tr>
        <th scope="col">Gender</th>
        <td><%= gender %></td>
    </tr>

    
    <tr>
        <th scope="col">Address</th>
        <td><%= address %></td>
    </tr>

    
    <tr>
        <th scope="col">Institute</th>
        <td><%= institute %></td>
    </tr>

    
    <tr>
        <th scope="col">Department</th>
        <td><%= department %></td>
    </tr>

    
    <tr>
        <th scope="col">Semester</th>
        <td><%= semester %></td>
    </tr>

  </table>
</div>
</div>



<%- include('footer') %>
